{
  "name": "delta-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "start": "vite start",
    "preview": "npm run build && wrangler pages dev --ip 0.0.0.0",
    "preview1": "wrangler pages dev --ip 0.0.0.0",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
